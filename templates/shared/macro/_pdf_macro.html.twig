{%- macro pdf_checkbox(checked, additionalStyles, attrs) -%}
	{% from 'shared/macro/_macro.html.twig' import render_attr %}
	<div class="pdf-checkbox {% if checked is same as(true) %}checked{% endif %}" style="{{ additionalStyles }}" {{ render_attr(attrs) }}>
		<div class="pdf-checkbox-inner">&#10004;</div>
	</div>
{%- endmacro -%}