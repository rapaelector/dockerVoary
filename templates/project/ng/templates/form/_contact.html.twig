<div class="card card card-outline card-app-secondary h-100" ng-if="data.newContact">
	<div class="card-header">
		<h3 class="card-title">Contact</h3>
		<!-- /.card-tools -->
	</div>
	<!-- /.card-header for interlocuteur -->
	<div class="card-body">
		<div class="row">
			<div class="card-body">
				<div class="row">
					<div class="col-sm-6">
						<div class="form-group">
							<label for="project_contact_lastName">Nom</label>
							<input type="text" ng-model="project.contact.lastName" class="form-control">
						</div>
					</div>
					<div class="col-sm-6">
						<div class="form-group">
							<label for="project_contact_firstName">Prénom</label>
							<input type="text" ng-model="project.contact.firstName" class="form-control">
						</div>
					</div>
					<div class="col-sm-6">
						<div class="form-group">
							<label for="project_contact_email">Email</label>
							<input type="text" ng-model="project.contact.email" class="form-control">
						</div>
					</div>
					<div class="col-sm-6">
						<div class="form-group">
							<label for="project_contact_phone">Téléphone</label>
							<input type="text" ng-model="project.contact.phone" class="form-control">
						</div>
					</div>
					<div class="col-sm-6">
						<div class="form-group">
							<label for="project_contact_job">Fonction</label>
							<input type="text" ng-model="project.contact.job" class="form-control">
						</div>
					</div>
					<div class="col-sm-6">
						<div class="form-group">
							<label for="project_contact_fax">Fax</label>
							<input type="text" ng-model="project.contact.fax" class="form-control">
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
	<!-- /.card-body -->
</div>
